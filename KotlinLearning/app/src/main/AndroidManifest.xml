<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kotlin.learning">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FiboNacciActivity"
            android:label="Fibonacci"/>
        <activity android:name=".PrimeNumber"
            android:label="PrimeNumber"/>
        <activity android:name=".Palindrome"
            android:label="Palindrome"/>
        <activity android:name=".NumberPalindrome"
            android:label="NumberPalindrome"/>
        <activity android:name=".ArmstrongNumber"
            android:label="Armstrong Number"/>
    </application>

</manifest>